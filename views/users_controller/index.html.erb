<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Users</title>
  </head>
  <body>
    <h1>Users</h1>
    <ul>
      <% @users.each do |user| %>
        <li>
          <p>
            <%= user.username %>
          </p>
          <ul>
            <% user.cats.each do |cat| %>
              <li> <%= cat.name %> </li>
            <% end %>
          </ul>
        </li>
      <% end %>
    </ul>
  </body>
</html>
